#: pacaur:108
msgid "${colorW}Starting AUR upgrade...${reset}"
msgstr "${colorW}Début de la mise à jour depuis l'AUR...${reset}"

#: pacaur:118
msgid "${colorW}$i${reset} is ${colorY}not present${reset} in AUR -- skipping"
msgstr "${colorW}$i${reset} n'est ${colorY}pas${reset} dans l'AUR — sauté"

#: pacaur:133
msgid ""
"${colorW}${allaurpkgs[$i]} ${colorY}${allaurpkgsQver[$i]}${reset}: ignoring "
"package upgrade"
msgstr ""
"${colorW}${allaurpkgs[$i]} ${colorY}${allaurpkgsQver[$i]}${reset} : mise à "
"jour ignorée"

#: pacaur:155
msgid "${colorY}::${reset} $i is in IgnorePkg/IgnoreGroup. Install anyway?"
msgstr ""
"${colorY}::${reset} $i est dans IgnorePkg/IgnoreGroup. L'installer quand "
"même ?"

#: pacaur:156 pacaur:284 pacaur:299
msgid "${colorW}$i${reset}: ignoring package upgrade"
msgstr "${colorW}$i${reset} : mise à jour ignorée"

#: pacaur:172
msgid "$pkg downloaded to $builddir"
msgstr "$pkg téléchargé dans $builddir"

